{
	"name": "spring-javaformat-vscode-extension",
	"description": "Spring JavaFormat Visual Studio Code Extension",
	"displayName": "Spring JavaFormat",
	"version": "0.0.36-SNAPSHOT",
	"engines": {
		"vscode": "^1.75.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/spring-io/spring-javaformat.git"
	},
	"categories": [
		"Formatters"
	],
	"activationEvents": [
		"onCommand:mrphil.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "mrphil.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/vscode": "^1.75.0",
		"@types/glob": "^8.0.1",
		"@types/mocha": "^10.0.1",
		"@types/node": "16.x",
		"@typescript-eslint/eslint-plugin": "^5.49.0",
		"@typescript-eslint/parser": "^5.49.0",
		"eslint": "^8.33.0",
		"glob": "^8.1.0",
		"mocha": "^10.1.0",
		"typescript": "^4.9.4",
		"@vscode/test-electron": "^2.2.2"
	}
}